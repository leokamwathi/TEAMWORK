{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "es6": true,
        "jasmine": true
    },
    "extends": ["airbnb", "prettier"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "script"
    },
    "plugins": ["react", "prettier"],
    "rules": {
        "no-console": "off"
    }
}
